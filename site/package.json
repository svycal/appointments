{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "concurrently \"pnpm run start:css\" \"docusaurus start\"",
    "build": "pnpm run build:css && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "generate": "docusaurus clean-api-docs api && docusaurus gen-api-docs api",
    "typecheck": "tsc",
    "start:css": "pnpm dlx @tailwindcss/cli -i ./src/css/styles.css -o ./src/css/build.css --watch",
    "build:css": "pnpm dlx @tailwindcss/cli -i ./src/css/styles.css -o ./src/css/build.css"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-common": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "@tailwindcss/cli": "^4.0.15",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^4.0.15"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@types/react": "^18.3.27",
    "concurrently": "^9.1.2",
    "raw-loader": "^4.0.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
