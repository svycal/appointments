{
  "name": "@savvycal/appointments-core",
  "version": "1.0.0",
  "type": "module",
  "description": "JavaScript/TypeScript client library for the SavvyCal Appointments API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate-schema": "openapi-typescript https://api.savvycal.app/v1/spec --output ./src/schema.d.ts && prettier --write ./src/schema.d.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  },
  "keywords": [
    "savvycal",
    "appointments",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/savvycal/appointments-js.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0"
  }
}
