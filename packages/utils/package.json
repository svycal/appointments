{
  "name": "@savvycal/appointments-utils",
  "version": "1.0.0",
  "type": "module",
  "description": "JavaScript/TypeScript utilities for the SavvyCal Appointments components",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "savvycal",
    "appointments",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/svycal/appointments.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "dependencies": {
    "temporal-polyfill": "^0.2.5"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
