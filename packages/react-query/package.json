{
  "name": "@savvycal/appointments-react-query",
  "version": "1.0.0",
  "type": "module",
  "description": "A tiny wrapper around @tanstack/react-query for the SavvyCal Appointments API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "generate:query-hooks": "tsx scripts/generate-query-hooks.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "savvycal",
    "appointments",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/savvycal/appointments-js.git",
    "directory": "packages/react-query"
  },
  "license": "MIT",
  "devDependencies": {
    "@tanstack/react-query": "^5.90.11",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "openapi-fetch": "^0.15.0",
    "openapi-react-query": "^0.5.1",
    "openapi-typescript": "^7.10.1",
    "react": "^19.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@savvycal/appointments-core": "workspace:*"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  }
}
